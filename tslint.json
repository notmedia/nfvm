{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "**/*.d.ts",
      "**/*.js"
    ]
  },
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "eofline": true,
    "quotemark": [
      true,
      "single"
    ],
    "indent": false,
    "member-access": [
      true,
      "no-public"
    ],
    "ordered-imports": [
      true
    ],
    "max-line-length": [
      true,
      150
    ],
    "curly": false,
    "interface-name": [
      false
    ],
    "array-type": [
      false
    ],
    "no-object-literal-type-assertion": false,
    "no-empty-interface": false,
    "no-empty": false,
    "arrow-parens": false,
    "object-literal-sort-keys": true,
    "no-unused-expression": true,
    "max-classes-per-file": false,
    "variable-name": [
      false
    ],
    "one-line": [
      false
    ],
    "one-variable-per-declaration": [
      false
    ],
    "no-parameter-reassignment": true
  },
  "rulesDirectory": []
}
