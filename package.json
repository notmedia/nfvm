    "jsonfile": "^5.0.0",
    "prompts": "^2.2.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/node": "^12.7.1",
    "@types/yargs": "^13.0.2",
    "codacy-coverage": "^3.4.0",
    "jest": "^24.8.0",
    "jest-fixtures": "^0.6.0",
